<templates xml:space="preserve">

    <t t-name="awesome_owl.Counter">
        <div class="p-4">
            <h2>OWL Counter Example</h2>

            <p>Counter: <t t-out="state.value"/></p>

            <button class="btn btn-primary" t-on-click="increment">
                Increment
            </button>
        </div>
    </t>

</templates>
